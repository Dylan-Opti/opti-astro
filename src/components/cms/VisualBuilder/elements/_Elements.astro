---
import HeadingElement from './HeadingElement/HeadingElement.astro'
import CTAElement from './CTAElement/CTAElement.astro'
import ImageElement from './ImageElement/ImageElement.astro'
import VideoElement from './VideoElement/VideoElement.astro'
import ParagraphElement from './ParagraphElement/ParagraphElement.astro'
import ContactElement from './Contact/ContactElement.astro'
import EventElement from './EventElement/EventElement.astro'
import Card from './CardElement/Card.astro'
import Hero from './HeroElement/Hero.astro'
import ArticleListElement from './ArticlesList/ArticleListElement.astro'
const { contentPayload } = Astro.props
const { element, displaySettings, displayTemplateKey } = Astro.props.element
const elementType = element._metadata.types[0]
---

{elementType === 'HeadingElement' &&
<HeadingElement contentPayload={contentPayload} element={element} displaySettings={displaySettings} />}
{elementType === 'CTAElement' &&
<CTAElement contentPayload={contentPayload} element={element} displaySettings={displaySettings} />}
{elementType === 'ImageElement' &&
<ImageElement contentPayload={contentPayload} element={element} displaySettings={displaySettings} />}
{elementType === 'VideoElement' &&
<VideoElement contentPayload={contentPayload} element={element} displaySettings={displaySettings} />}
{elementType === 'ParagraphElement' &&
<ParagraphElement contentPayload={contentPayload} element={element} displaySettings={displaySettings} />}
{elementType === 'ContactElement' &&
<ContactElement contentPayload={contentPayload} element={element} displaySettings={displaySettings} />}
{elementType === 'EventElement' &&
<EventElement contentPayload={contentPayload} element={element} displaySettings={displaySettings} />}
{elementType === 'CardElement' &&
<Card contentPayload={contentPayload} element={element} displaySettings={displaySettings}
      displayTemplateKey={displayTemplateKey} />}
{elementType === 'HeroElement' &&
<Hero contentPayload={contentPayload} element={element} displaySettings={displaySettings} />}
{elementType === 'ArticleListElement' &&
<ArticleListElement contentPayload={contentPayload} element={element} displaySettings={displaySettings} />}

