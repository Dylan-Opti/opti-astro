---
import HeadingElement from "./HeadingElement/HeadingElement.astro";
import CTAElement from "./CTAElement/CTAElement.astro";
import ImageElement from "./ImageElement/ImageElement.astro";
import VideoElement from "./VideoElement/VideoElement.astro";
import ParagraphElement from "./ParagraphElement/ParagraphElement.astro";
const {element, displaySettings} = Astro.props.element;
const elementType = element._metadata.types[0];
---

{elementType === "HeadingElement" &&
<HeadingElement element={element} displaySettings={displaySettings}/>}
{elementType === "CTAElement" &&
<CTAElement element={element} displaySettings={displaySettings}/>}
{elementType === "ImageElement" &&
<ImageElement element={element} displaySettings={displaySettings}/>}
{elementType === "VideoElement" &&
<VideoElement element={element} displaySettings={displaySettings}/>}
{elementType === "ParagraphElement" &&
<ParagraphElement element={element} displaySettings={displaySettings}/>}
