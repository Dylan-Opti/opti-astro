---
import ArticleListElement from "./ArticlesList/ArticleListElement.astro";
import Card from "./CardElement/Card.astro";
import ContactElement from "./Contact/ContactElement.astro";
import CTAElement from "./CTAElement/CTAElement.astro";
import EventElement from "./EventElement/EventElement.astro";
import HeadingElement from "./HeadingElement/HeadingElement.astro";
import Hero from "./HeroElement/Hero.astro";
import ImageElement from "./ImageElement/ImageElement.astro";
import ParagraphElement from "./ParagraphElement/ParagraphElement.astro";
import VideoElement from "./VideoElement/VideoElement.astro";
const { contentPayload } = Astro.props;
const { component, displaySettings, displayTemplateKey } = Astro.props.element;
const element = component;
const elementType = element._metadata.types[0];
---

{
  elementType === "HeadingElement" && (
    <HeadingElement
      contentPayload={contentPayload}
      element={element}
      displaySettings={displaySettings}
    />
  )
}
{
  elementType === "CTAElement" && (
    <CTAElement
      contentPayload={contentPayload}
      element={element}
      displaySettings={displaySettings}
    />
  )
}
{
  elementType === "ImageElement" && (
    <ImageElement
      contentPayload={contentPayload}
      element={element}
      displaySettings={displaySettings}
    />
  )
}
{
  elementType === "VideoElement" && (
    <VideoElement
      contentPayload={contentPayload}
      element={element}
      displaySettings={displaySettings}
    />
  )
}
{
  elementType === "ParagraphElement" && (
    <ParagraphElement
      contentPayload={contentPayload}
      element={element}
      displaySettings={displaySettings}
    />
  )
}
{
  elementType === "ContactElement" && (
    <ContactElement
      contentPayload={contentPayload}
      element={element}
      displaySettings={displaySettings}
    />
  )
}
{
  elementType === "EventElement" && (
    <EventElement
      contentPayload={contentPayload}
      element={element}
      displaySettings={displaySettings}
    />
  )
}
{
  elementType === "CardElement" && (
    <Card
      contentPayload={contentPayload}
      element={element}
      displaySettings={displaySettings}
      displayTemplateKey={displayTemplateKey}
    />
  )
}
{
  elementType === "HeroElement" && (
    <Hero
      contentPayload={contentPayload}
      element={element}
      displaySettings={displaySettings}
    />
  )
}
{
  elementType === "ArticleListElement" && (
    <ArticleListElement
      contentPayload={contentPayload}
      element={element}
      displaySettings={displaySettings}
    />
  )
}
