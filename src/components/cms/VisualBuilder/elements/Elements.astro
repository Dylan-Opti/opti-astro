---
import HeadingElement from "./HeadingElement/HeadingElement.astro";
import CTAElement from "./CTAElement/CTAElement.astro";
import ImageElement from "./ImageElement/ImageElement.astro";
const {element} = Astro.props.element;
const elementType = element._metadata.types[0];
---

{elementType === "HeadingElement" &&
<HeadingElement element={element}/>}
{elementType === "CTAElement" &&
<CTAElement element={element}/>}
{elementType === "ImageElement" &&
<ImageElement element={element}/>}
